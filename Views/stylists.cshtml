@{
    Layout="Shared/_Layout.cshtml";
}
@if(Model.Count == 0)
{
    <h5>There are no stylists listed</h5>

}
else
{
    <div class="col s4">
    @foreach(var entry in Model)
    {
        <div class="stylist-holder">
        <a href="#" id="@entry.GetName()"><h5>@entry.GetName()</h5></a>
        </div>

    }
    </div>
}
